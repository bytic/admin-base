{
  "name": "bytic/admin-base",
  "type": "library",
  "description": "Admin interface package for bytic framework",
  "homepage": "https://github.com/bytic/admin-base",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\AdminBase\\": "src",
      "ByTIC\\AdminBase\\Tests\\Helper\\": "tests/_support/Helper"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\AdminBase\\Tests\\": "tests/src",
      "ByTIC\\AdminBase\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^8.0",
    "bytic/package-base": "^1.0",
    "bytic/contracts": "^1.0",
    "bytic/icons": "^1.0",
    "bytic/view": "^0.9|^1.0|^2.0",
    "bytic/orm": "^0.9|^1.0",
    "bytic/html-helpers": "^1.0.8",
    "bytic/seo-meta": "^1.0"
  },
  "require-dev": {
    "bytic/translation": "^0.9|^1.0",
    "bytic/mvc": "^0.9|^1.0",
    "bytic/codeception": "^4.0",
    "bytic/phpqatools": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true,
      "bytic/phpqatools": true
    }
  }
}
